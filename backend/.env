# Backend environment for SQLite
NODE_ENV=development
PORT=5001

# Prisma SQLite database file (relative to prisma/schema.prisma)
DATABASE_URL="file:./dev.db"

# JWT configuration (generate a secure secret via scripts/generate-jwt-secret.js)
JWT_SECRET=change_me_to_a_secure_random_string
JWT_EXPIRES_IN=7d

# Admin credentials (update for production)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123